<!-- Aprender -->

	<category>
		<pattern> NO CONOZCO </pattern>
		<template>No conozco la respuesta a esa pregunta ¿quieres que la guarde?</template>
	</category>

	<category>
		<pattern>*</pattern>
		<template><think><set name="pregunta"><star/></set></think>
			<srai> NO CONOZCO </srai>
		</template>
	</category>

	<category>
		<pattern> SI </pattern>
		<that> No conozco * </that>
		<template>
			Qué respuesta quieres que guarde para "<get name="pregunta"/>"?
		</template>
	</category>

	<category>
		<pattern>*</pattern>
		<that> * respuesta quieres que guarde * </that>
		<template>
			<think><set name="respuesta"><star/></set></think>
			<learn>
				<category>
					<pattern>
						<eval><get name="pregunta"/></eval>
					</pattern>
					<template>
						<eval><get name="respuesta"></get></eval>
					</template>

				</category>
			</learn>
			(Respuesta guardada "<get name="pregunta"/> -> <get name="respuesta"/>")
		</template>
	</category>

