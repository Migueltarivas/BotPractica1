<category>
	<pattern>Cuenta hasta 10</pattern>
	<template>
		<think>
			<set var="contador">primero</set>
		</think>
		<condition var="contador">
			<li><value>decimo</value><get var="contador"/></li>
			<li><get var="contador"/>
			<think>
						<set var="contador">
							<map name="siguiente"><get var="contador"/></map>
						</set>
					</think><loop/></li>
		</condition>
				
	</template>
